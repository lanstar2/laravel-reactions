{
    "name": "kesty/laravel-reactions",
    "type": "library",
    "description": "",
    "keywords": [
        "laravel",
        "package",
	"reactions",
	"like",
	"love",
	"eloquent",
	"trait"
    ],
    "homepage": "https://github.com/olubunmitosin/laravel-reactions",
    "license": "MIT",
    "authors": [
        {
            "name": "Olubunmi Tosin",
            "email": "vickkycodegenius@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^8.0",
        "illuminate/database": "~5.5|~5.6|~5.7|~5.8|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "~5.5|~5.6|~5.7|~5.8|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.9",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^7.6",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Kesty\\LaravelReactions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kesty\\LaravelReactions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR4 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR4 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
